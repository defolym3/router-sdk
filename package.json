{
  "name": "@defolym3/router-sdk",
  "version": "0.1.0",
  "author": "@Uniswap",
  "module": "dist/router-sdk.esm.js",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepublishOnly": "npm run build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "dependencies": {
    "@defolym3/sdk-core": "^1.3.8",
    "@defolym3/v2-sdk": "^1.2.3",
    "@defolym3/v3-sdk2": "^3.11.5",
    "@uniswap/swap-router-contracts": "1.1.0",
    "jsbi": "^3.1.4"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^8.2.6",
    "husky": "^8.0.3",
    "size-limit": "^8.2.6",
    "tsdx": "^0.14.1",
    "tslib": "^2.6.2",
    "typescript": "^5.1.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "size-limit": [
    {
      "path": "dist/router-sdk.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/router-sdk.esm.js",
      "limit": "10 KB"
    }
  ]
}
